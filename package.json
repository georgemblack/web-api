{
  "name": "web-api",
  "type": "module",
  "main": "app.js",
  "repository": "git@github.com:georgemblack/web-api.git",
  "author": "georgemblack <contact@george.black>",
  "license": "MIT",
  "targets": {
    "default": {
      "context": "node",
      "engines": {
        "node": ">= 18"
      },
      "outputFormat": "esmodule"
    }
  },
  "scripts": {
    "start": "node dist/app.js",
    "build": "parcel build src/app.ts  --target default",
    "format": "prettier ./src --write",
    "lint": "eslint ./src --fix",
    "tsc": "tsc --noEmit"
  },
  "dependencies": {
    "@google-cloud/firestore": "^4.15.1",
    "@markdoc/markdoc": "^0.1.4",
    "config": "^3.3.6",
    "express": "^4.17.3",
    "google-auth-library": "^7.10.0",
    "jsonwebtoken": "^9.0.0",
    "lodash.isequal": "^4.5.0",
    "node-fetch": "^2.6.1",
    "pino-http": "^5.8.0",
    "rate-limiter-flexible": "^2.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/config": "^3.3.0",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node-fetch": "^2.6.2",
    "@types/pino-http": "^5.8.1",
    "@types/react": "^18.0.29",
    "@types/react-dom": "^18.0.11",
    "@types/uuid": "^9.0.1",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.1.1",
    "parcel": "^2.8.3",
    "prettier": "^2.4.1",
    "typescript": "^5.0.2"
  }
}
